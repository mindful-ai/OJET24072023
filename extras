var tablegen;
        $(document).ready(function(){
            tablegen = function(val){
                if(val != ''){
                    $("#table").empty();
                    for(i = 1; i <= 10; i++){
                        var child = $("<h3></h3>").text(val + " X " + i + " = " + (val*i));
                        $("#table").append(child);
                    }
                }    
            }
        })
